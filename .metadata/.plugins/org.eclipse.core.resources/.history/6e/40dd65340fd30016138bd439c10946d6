package poomine;

import java.util.ArrayList;
import java.util.Random;

public class Words {

public ArrayList<String> word;
    
    public Words () {
        word = new ArrayList<String>();
        
		word.add("ATMOSPHERE");
		word.add("MAGNIFICENT");
		word.add("BLACKBOARD");
		word.add("PENGUIN");
		word.add("MUSHROOM");
		
    }


	    public String getRandom() {
	        return word.get(new Random().nextInt(word.size()));}
		
	    
}

